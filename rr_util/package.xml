<?xml version="1.0"?>
<package format="2">
    <name>rr_util</name>
    <version>0.0.0</version>
    <description>The RoboRacing package</description>

    <maintainer email="nfbartholomai@gmail.com">nico</maintainer>

    <license>MIT</license>

    <url type="website">http://robojackets.org</url>
    <url type="website">http://wiki.robojackets.org/w/RoboRacing</url>
    <url type="website">http://www.github.com/robojackets/roboracing-software</url>


    <buildtool_depend>ament_cmake</buildtool_depend>

    <depend>rclcpp</depend>
    <depend>rclpy</depend>
    <depend>std_msgs</depend>
    <depend>rr_msgs</depend>
    <depend>tf2_ros</depend>
    <depend>nav2_msgs</depend>
