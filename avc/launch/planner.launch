<launch>

    <!-- <node name="planner" pkg="avc" type="planner" output="screen" respawn="false"> -->
    <node name="planner" pkg="avc" type="planner" output="screen" >
        <param name="steer_stddev"     type="double" value="10" />
        <param name="max_steer_angle"  type="double" value="30" />
        <param name="path_iterations"  type="int"    value="300" />
        <param name="max_speed"        type="double" value="1.5" />
        <param name="path_time"        type="double" value="4.0" />
        <param name="time_increment"   type="double" value="0.12" />
        <param name="action_time"      type="double" value="0.75" />
        <param name="collision_radius" type="double" value="0.4" />
    </node>

</launch>
